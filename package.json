{
  "name": "ecp-portal-static",
  "version": "1.0.0",
  "description": "webpact + react + redux + es6 脚手架",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "lint": "eslint src",
    "test": "make test",
    "start": "npm run dev:local",
    "dev:local": "cross-env NODE_ENV=development node build/webpack.dev.js --hot",
    "dev:test": "cross-env NODE_ENV=development node build/webpack.dev.js --hot --deploy",
    "release": "npm run deploy:online",
    "deploy:test": "cross-env NODE_ENV=production node build/webpack.release.js --watch --uglify --deploy=test",
    "deploy:online": "cross-env NODE_ENV=production node build/webpack.release.js --uglify --deploy=online"
  },
  "repository": {
    "type": "git",
    "url": "git+git@git.yonyou.com:shangyj5/ecp-portal-static.git"
  },
  "keywords": [
    "webpack",
    "react",
    "es6",
    "babel",
    "redux",
    "boilerplate"
  ],
  "author": "ecp",
  "license": "MIT",
  "devDependencies": {
    "antd": "^2.7.4",
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.3.0",
    "babel-plugin-import": "^1.1.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "es6-promise": "^4.1.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "ftp": "^0.3.10",
    "glob": "^7.0.5",
    "html-webpack-plugin": "^2.22.0",
    "image-webpack-loader": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.1.1",
    "js-cookie": "^2.1.3",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "lodash": "^4.17.4",
    "md5": "^2.1.0",
    "object-assign": "^4.1.1",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "react-hot-loader": "^1.3.0",
    "react-router-redux": "^4.0.8",
    "redux-logger": "^2.8.2",
    "standard-error": "^1.1.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-md5-hash": "0.0.5"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  }
}
